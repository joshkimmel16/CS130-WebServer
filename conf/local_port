port 8080;
root .;
threadCount 10;
router {
    handler echo {
        location /echo;
    }
    handler status {
        location /status;
    }
    handler static1 {
        location /static1;
        root resources/static1;
    }
    handler static2 {
        location /static2;
        root resources/static2;
    }
    handler proxy {
        port 8080; # if port number is missing here, the default should be 8080
        location /ucla;
        root www.ucla.edu;
    }
}